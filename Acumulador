/* ERCIK SAUL GARCIA HERNANDEZ
PRACTICA #3 -Suma de 0 a 100*/
#include <stdio.h>

    int i;
    int suma = 0;

    int main ()
    {
        for (i = 0;  i <= 100; i++)
        {
            suma = suma + i;
        }

        printf("La suma de los numeros es de:: %d\n", suma);
        return 0;
    }
